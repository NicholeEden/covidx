


<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta http-equiv="x-ua-compatible" content="ie=edge" />

	<title>@ViewData["Title"]</title>
	<partial name="CovidXAdminLte/_Styles" />
</head>
<body class="hold-transition sidebar-mini">
	<div class="wrapper">

		<partial name="CovidXAdminLte/_TopNavigation" />



		<partial name="CovidXAdminLte/_MainNavigation" />
		<div class="content-wrapper">

			<div class="content">
				<div class="container-fluid">
					<!--Toaster partial-->
					<partial name="_NotificationPartial"/>
					@RenderBody()
				</div>
			</div>
		</div>
		<aside class="control-sidebar control-sidebar-dark">
			<div class="p-3">
				<h5>Title</h5>
				<p>Sidebar content</p>
			</div>
		</aside>
		<partial name="CovidXAdminLte/_Footer" />
	</div>
	<partial name="CovidXAdminLte/_Scripts" />
</body>
  @await RenderSectionAsync("Scripts", required: false)
</html>



